{
  "audio": "none",
  "bootloader": "systemd-bootctl",
  "custom-commads": [],
  "dry-run": false,
  "hostname": "arcren",
  "kernels": [
    "linux-zen"
  ],
  "keyboard-layout": "pl",
  "mirror-region": {
    "Poland" : {
      "http://ftp.icm.edu.pl/pub/Linux/dist/archlinux/$repo/os/$arch": true,
      "http://mirror.onet.pl/pub/mirrors/archlinux/$repo/os/$arch": true
    },
    "Worldwide": {
      "https://mirror.rackspace.com/archlinux/$repo/os/$arch": true
    }
  },
  "nic": {
    "NetworkManager": true,
    "nic": "Use NetworkManager (necessary to configure internet graphically in GNOME and KDE)"
  },
  "ntp": true,
  "packages": [
    "git",
    "wget"
  ],
  "profile": "minimal",
  "script": "guided",
  "superusers": {
    "ren": {
      "!password": "pass"
    }
  },
  "swap": false,
  "sys-encoding": "utf-8",
  "sys-language": "en_US",
  "timezone": "Europe/Warsaw",
  "version": "2.3.2"
}
